<link rel="stylesheet" href="/scss/signIn.css">

<h1>Sign in</h1>
<div id="signcontainer">
<form action="/users/create-session" method="POST">
    <input type="email" name="email" placeholder="Email" required class="email-input">
    <br>
    <input type="password" name="password" placeholder="Password" required class="email-input">
    <br>
    <input type="submit" value="Sign In" id="s-btn">


</form>
<br>
<p>
    <a href="auth/google">Google SignIn/SignUp</a>
</p>
    <a href="/users/forgot-password">Forgot Password?</a>
</p>
</div>
